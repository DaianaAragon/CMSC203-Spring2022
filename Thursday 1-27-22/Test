public class Test {

	public static void main(String[] args) {
		//1.- Create three variables for Age, Name and Address.- 		
		//2.- Initialize them 
		//3.- Display them in one line
		
		int age;
		String name, address;
		//String address;
		
		age = 20;
		name = "John";
		address = "123 Main St.";
		
		
		System.out.print(age + " " + name);
		System.out.print(address);
		
		System.out.println(age + " " + name + " \n" + address);
		System.out.print(age + " " + name + " " + address);
		
	}

}
